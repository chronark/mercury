{
  "overwrite": true,
  "schema": [
    {
      "https://graphql.fauna.com/graphql": {
        "headers": {
          "Authorization": "Bearer ${FAUNA_ADMIN_KEY}"
        }
      }
    }
  ],
  "generates": {
    "pkg/graphql/gen/index.ts": {
      "documents": ["./pkg/graphql/operations/*.gql"],
      "plugins": ["typescript", "typescript-react-apollo", "typescript-operations"]
    }
  }
}
