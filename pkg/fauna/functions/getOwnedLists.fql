Update(
  Function("getOwnedLists"), {
    role: Role("authenticated_user"),
    body: Query(
      Lambda(
        [], 
        Select(
          ["data"], 
          Map(
            Paginate(
              Match(
                Index("lists_by_owner"), 
                CurrentIdentity()
              )
            ),
            Lambda("listRef", Get(Var("listRef")))
          )
        )
      )
    ) 
  }
)
