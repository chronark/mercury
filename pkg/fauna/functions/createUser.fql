Update(Function("create_user"), {
  body: Query(
    Lambda(
      ["input"],
      Create(Collection("users"), {
        data: {
          username: Select("username", Var("input")),
          tier: Select("tier", Var("input")),
        },
        credentials: {
          password: Select("password", Var("input")),
        },
      }),
    ),
  ),
})